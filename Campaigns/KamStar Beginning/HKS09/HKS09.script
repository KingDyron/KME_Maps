	gGroupsKilled : Boolean;
	dialogEnd : Integer;
	
procedure SpawnRebels;
var I : Integer;
	arr : array of Integer;
	Pos : TKMPoint;
begin

	arr := S.PlayerGetAllHouses(1);
	for I := 0 to high(arr) do 
		If I mod 2 = 0 then
		begin
			Pos := S.HousePosition(arr[I]);
			case S.HouseTypeEx(arr[I]) of
			
				htTownhall, htBarracks,
				htSmallStore,
				htStore : A.GiveGroup(3, 16, Pos.X, Pos.Y + 2, 4, 4, 2);//sword fighter
				
				htInn, htSchool, htIronMine, htGoldMine, htCoalMine, 
				htSawMill: A.GiveGroup(3, 19, Pos.X, Pos.Y + 2, 4, 3, 3);//Pikemans
				htWoodcutters, htQuarry, htCollectors: A.GiveGroup(3, 16, Pos.X, Pos.Y + 2, 4, 2, 2);//militia
				htPalace : ;
				htWall..htWall5, htWell: ;
				else 	A.GiveGroup(3, 24, Pos.X, Pos.Y + 2, 4, 2, 2);//rebels	
			end;
		end;
	
end;

procedure CheckGroups;
var I, J : Integer;
	groupsKilled : Boolean;
begin
	If gGroupsKilled then 
		Exit;
	for I := 0 to high(gGroupsToKill) do
	begin
		groupsKilled := true;
		for J := 0 to high(gGroupsToKill[I]) do
			If not S.GroupDead(gGroupsToKill[I][J]) then 
				groupsKilled := false;
		If groupsKilled then 
		begin
			//A.ShowMsg(0, 'Groups killed: '  + IntToStr(I));
			gGroupsKilled := true;
			SpawnRebels;
			Exit;
		end;
	end;
end;

procedure OnTick;
begin
	If S.GameTime mod 10 = 0 then 
		CheckGroups;
	If S.GameTime = dialogEnd then 
		A.ShowMsg(0, IntToMsg(58));
end;

