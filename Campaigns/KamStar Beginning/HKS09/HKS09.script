procedure PlaceHouses(PL : Integer);
var I : Integer;
	tmpStats : TKMHouseStats;
begin
	If length(CampaignData.HousesBuilt) = 0 then
		PlaceDefault(PL);
		
	A.GiveHouse(PL, 51, 65 + OFFSET_X, 42 + OFFSET_Y);
	A.PlanConnectRoad(PL, 65 + OFFSET_X, 43 + OFFSET_Y, 89 + OFFSET_X, 46 + OFFSET_Y, true);
	A.PlayerAddWorkers(PL, false);	
	
	A.GiveGroup(PL, 0, 65 + OFFSET_X, 43 + OFFSET_Y, 0, 20, 1);
	A.GiveGroup(PL, 0, 90 + OFFSET_X, 49 + OFFSET_Y, 0, 20, 1);
	A.GiveGroup(PL, 0, 61 + OFFSET_X, 53 + OFFSET_Y, 0, 20, 1);
	A.GiveGroup(PL, 0, 61 + OFFSET_X, 41 + OFFSET_Y, 0, 20, 1);
	A.GiveGroup(PL, 9, 54 + OFFSET_X, 43 + OFFSET_Y, 0, 10, 1);
	
	for I := 0 to high(CampaignData.HousesBuilt) do 
	begin
		tmpStats := CampaignData.HousesBuilt[I];
		tmpStats.X := tmpStats.X + OFFSET_X;
		tmpStats.Y := tmpStats.Y + OFFSET_Y;
		A.GiveHouseFromStats(PL, tmpStats);
	end;
	for I := 0 to high(CampaignData.FieldsBuilt) do 
		A.MapTileFieldSet(CampaignData.FieldsBuilt[I].X + OFFSET_X, CampaignData.FieldsBuilt[I].Y + OFFSET_Y, PL, CampaignData.FieldsBuilt[I].FT);	
		
end;
	
procedure OnMissionStart;
begin
	gPanelsRefreshRate := 10;
	Hero_AddFromLoc(114, 31, 4, 'Eryt', false);
	Hero_SetStats('Eryt', 3, 100, 0, 3, 24, 9, 2, 1);
	Hero_AddFromLoc(112, 32, 3, 'Malcolm', false);
	Hero_SetStats('Malcolm', 5, 80, 30, 5, 28, 7, 2, 30);
	OverlayDialogSetup;	
	
	A.CinematicStart(PLAYER);
	Dialog_SetStartTime(10);
	Dialog_Loc(113, 32);
	Dialog_Add(50, 25, Hero_IDByName('Eryt'));
	Dialog_Add(51, 15, Hero_IDByName('Malcolm'));
	Dialog_Add(52, 40, Hero_IDByName('Eryt'));
	Dialog_Add(53, 40, Hero_IDByName('Malcolm'));
	Dialog_Add(54, 55, Hero_IDByName('Eryt'));
	Dialog_Add(55, 50, Hero_IDByName('Malcolm'));
	Dialog_Add(56, 40, Hero_IDByName('Eryt'));
	Dialog_Add(57, 20, Hero_IDByName('Malcolm'));
	dialogEnd := Dialog_EndTime;
	
	gGroupsToKill[0]:= [S.GroupAt(110, 118), S.GroupAt(120, 111)];
